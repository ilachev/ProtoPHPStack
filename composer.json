{
    "require": {
        "php": "^8.4",
        "nikic/fast-route": "^1.3",
        "nyholm/psr7": "^1.8",
        "psr/container": "^2.0",
        "psr/http-server-middleware": "^1.0",
        "spiral/roadrunner-http": "^3.5",
        "spiral/roadrunner-worker": "^3.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
